    "build-all": "electron-packager . --ignore=\"\\.git(ignore|modules)\" --ignore=\"\\.eslintrc.json\" --ignore=\"(?:index|popup)Chrome(?:.js)*\" --ignore=\"manifest\\.json\" --ignore=\"\\.vscode\" --ignore=\"\\.github\" --ignore=\"out\" --ignore=\"JSDoc.js\" --out=./out --overwrite --all",
